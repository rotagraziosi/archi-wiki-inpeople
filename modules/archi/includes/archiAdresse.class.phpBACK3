			$mail = new mailObject();
			$mail->sendMailToAdministrators($mail->getSiteMail(),'Un utilisateur a ajouté un commentaire',$message," AND alerteCommentaires='1' ");

			// remise a zero des variables en post sinon on va reafficher les infos
